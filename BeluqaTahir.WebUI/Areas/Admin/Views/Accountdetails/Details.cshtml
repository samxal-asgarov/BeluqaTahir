@model BeluqaTahir.Domain.Model.Entity.Accountdetails

@{
    ViewData["Title"] = "Details";
}

<h1 style="text-align:center">Accountdetails Details</h1>


<div class="white_card_body">
    <div class="card-body">
        <div class="form-row">
          

            <div class="col-8">

                <div class="form-group col-12">
                    <label asp-for="Email" class="control-label"></label>
                    <p class="form-control">@Model.Email</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Twwiter" class="control-label"></label>
                    <p class="form-control textarea ">@Model.Twwiter</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Facebook" class="control-label"></label>
                    <p class="form-control textarea">@Model.Facebook</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Instagram" class="control-label"></label>
                    <p class="form-control textarea">@Model.Instagram</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Linkedin" class="control-label"></label>
                    <p class="form-control textarea">@Model.Linkedin</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Phone" class="control-label"></label>
                    <p class="form-control textarea">@Model.Phone</p>
                </div>

                <div class="form-group col-12">
                    <label asp-for="Address" class="control-label"></label>
                    <p class="form-control textarea">@Model.Address</p>
                </div>




            </div>
        </div>
    </div>

</div>



<a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
<a class="btn btn-outline-dark" asp-action="Index">Back to List</a>


